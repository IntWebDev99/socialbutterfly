ame=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#2f1893\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row justify-content-center text-center\">\n           <div class=\"col-xl-7 col-lg-8 col-md-10 icons\" mbr-if=\"iconsAmount > 0\" mbr-list=\"\" mbr-list-grow=\"iconsAmount\">\n                  <span mbr-icon class=\"mbr-iconfont socicon-instagram socicon\" style=\"font-size: 46px; color: rgb(47, 24, 147); fill: rgb(47, 24, 147);\"></span>\n                  <a href=\"http://m.me/tiner.tiner.520\" target=\"_blank\"><span mbr-icon class=\"mbr-iconfont socicon-facebook socicon\" style=\"font-size: 46px; color: rgb(47, 24, 147); fill: rgb(47, 24, 147);\"></span></a>\n                  <span mbr-icon class=\"mbr-iconfont mbri-video\" style=\"font-size: 44px; color: rgb(47, 24, 147); fill: rgb(47, 24, 147);\"></span>\n              <a href=\"mailto:jktiner101@integratedwebsitedevelopment.company\"><span mbr-icon class=\"mbr-iconfont mbri-letter\" style=\"font-size: 46px; color: rgb(47, 24, 147); fill: rgb(47, 24, 147);\"></span></a></div>\n            <div class=\"col-xl-7 col-lg-8 col-md-10\">\n               <h2 class=\"mbr-section-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">Fonts and Icons</h2>\n            </div>\n            <div class=\"col-xl-7 col-lg-8 col-md-9 col-sm-10\">\n               <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"displ